  "compilerOptions": {
    "outDir": "./dist/vsing-server",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "types": ["node"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "files": ["server.ts"],
  "angularCompilerOptions": {
    "entryModule": "src/app/app.server.module#AppServerModule"
  }
}